{
  "name": "vuejs-skeleton",
  "version": "0.5.0",
  "license": "MIT",
  "private": true,
  "description": "vue3 frontend with vite and tailwindcss, a vuejs skeleton",
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "cypress open-ct",
    "test:unit:ci": "cypress run-ct --quiet --reporter spec",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext .vue,.js,.ts --ignore-path .gitignore --fix"
  },
  "dependencies": {
    "@headlessui/vue": "^1.5.0",
    "@heroicons/vue": "^1.0.5",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/js-cookie": "^3.0.1",
    "@vuedx/typescript-plugin-vue": "^0.7.4",
    "axios": "^0.24.0",
    "js-cookie": "^3.0.1",
    "pinia": "^2.0.11",
    "pinia-plugin-persist": "^0.0.92",
    "tailwindcss-hero-patterns": "^0.0.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.2",
    "@cypress/vue": "^3.1.1",
    "@iconify/json": "^2.1.4",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "animated-tailwindcss": "^2.6.1",
    "autoprefixer": "^10.4.2",
    "cypress": "^9.5.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "postcss": "^8.4.4",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "vite": "^2.8.4",
    "vue-tsc": "^0.31.4"
  }
}
