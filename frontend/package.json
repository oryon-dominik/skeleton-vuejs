{
  "name": "vuejs-skeleton",
  "version": "0.5.0",
  "license": "MIT",
  "private": true,
  "description": "vue3 frontend with vite and tailwindcss, a vuejs skeleton",
  "scripts": {
    "dev": "vite",
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "test:unit": "cypress open-ct",
    "test:unit:ci": "cypress run-ct --quiet --reporter spec",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint --ext .vue,.js,.ts,.cjs --ignore-path .gitignore --fix ."
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.3",
    "@heroicons/vue": "^2.0.12",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/js-cookie": "^3.0.2",
    "@vuedx/typescript-plugin-vue": "^0.7.5",
    "axios": "^1.1.2",
    "js-cookie": "^3.0.1",
    "pinia": "^2.0.22",
    "pinia-plugin-persist": "^1.0.0",
    "tailwind-scrollbar": "^2.0.1",
    "tailwindcss-hero-patterns": "^0.0.1",
    "uuid": "^9.0.0",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^3.3.0",
    "@cypress/vue": "^4.2.0",
    "@iconify/json": "^2.1.119",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/compiler-sfc": "^3.2.40",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "animated-tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.9.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "vite": "^3.1.6",
    "vitest": "^0.24.0",
    "vue-tsc": "^1.0.0"
  }
}
